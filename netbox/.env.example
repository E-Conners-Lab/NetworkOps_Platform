# NetBox Docker Environment Variables
# Copy this file to .env and fill in the values

# PostgreSQL password
POSTGRES_PASSWORD=change-me-postgres-password

# NetBox secret key (generate with: python3 -c "import secrets; print(secrets.token_urlsafe(50))")
NETBOX_SECRET_KEY=change-me-generate-a-50-char-secret-key

# NetBox superuser credentials
NETBOX_SUPERUSER=admin
NETBOX_SUPERUSER_EMAIL=admin@example.com
NETBOX_SUPERUSER_PASSWORD=admin

# API Token for automation (generate a UUID or secure string)
# This token will be created automatically for the superuser
NETBOX_API_TOKEN=change-me-generate-api-token
